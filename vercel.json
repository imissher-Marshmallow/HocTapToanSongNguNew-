{
  "version": 2,
  "name": "stem-project",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "stem-project/build",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
